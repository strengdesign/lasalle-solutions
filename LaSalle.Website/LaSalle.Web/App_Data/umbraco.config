        <Item id="1715" key="63ab11ae-3659-4b17-bab3-d800786bec2c" parentID="1087" level="4" creatorID="6" sortOrder="21" createDate="2016-05-16T19:06:07" updateDate="2017-02-15T15:22:45" nodeName="LaSalle Solutions named 2016 Cisco Services Partner of the Year, Central Area" urlName="lasalle-solutions-named-2016-cisco-services-partner-of-the-year-central-area" path="-1,1056,1071,1087,1715" isDoc="" nodeType="1130" creatorName="tarek.wiley" writerName="Beth Kirshenberg" writerID="2" template="1135" nodeTypeAlias="Item">
