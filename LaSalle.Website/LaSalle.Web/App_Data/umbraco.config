    <GeneralInterior id="1318" key="617c9ace-6f89-447b-a679-87cd63e9ad8b" parentID="1056" level="2" creatorID="0" sortOrder="8" createDate="2014-08-05T16:25:01" updateDate="2014-10-02T10:23:22" nodeName="Newsletter" urlName="newsletter" path="-1,1056,1318" isDoc="" nodeType="1053" creatorName="Streng Design" writerName="Streng Design" writerID="0" template="1066" nodeTypeAlias="GeneralInterior">
