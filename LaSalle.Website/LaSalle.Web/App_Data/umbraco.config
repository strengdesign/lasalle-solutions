    <GeneralInterior id="1915" key="7a0a5bf5-5eea-463a-9502-b19936b516cf" parentID="1056" level="2" creatorID="0" sortOrder="41" createDate="2017-02-22T19:06:04" updateDate="2017-03-02T12:03:08" nodeName="Critical Component Issue" urlName="critical-component-issue" path="-1,1056,1915" isDoc="" nodeType="1053" creatorName="Streng Design" writerName="tarek.wiley" writerID="6" template="1050" nodeTypeAlias="GeneralInterior">
