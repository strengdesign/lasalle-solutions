        <Item id="1384" key="3c6c7380-0290-44c1-b8de-5cf731dfd468" parentID="1131" level="4" creatorID="0" sortOrder="0" createDate="2014-08-07T08:52:40" updateDate="2017-02-24T10:53:25" nodeName="LaSalle Recognized with Cisco Customer Satisfaction Excellence for 38th Consecutive Quarter" urlName="lasalle-recognized-with-cisco-customer-satisfaction-excellence-for-38th-consecutive-quarter" path="-1,1056,1071,1131,1384" isDoc="" nodeType="1130" creatorName="Streng Design" writerName="tarek.wiley" writerID="6" template="1135" nodeTypeAlias="Item">
