    <GeneralInterior id="1802" key="d8042924-1fab-4c44-b613-d46829f04f1e" parentID="1056" level="2" creatorID="6" sortOrder="39" createDate="2016-09-02T11:45:30" updateDate="2016-09-02T11:53:43" nodeName="5 Important Takeaways from the New FASB Lease Accounting Standards" urlName="5-important-takeaways-from-the-new-fasb-lease-accounting-standards" path="-1,1056,1802" isDoc="" nodeType="1053" creatorName="tarek.wiley" writerName="tarek.wiley" writerID="6" template="1066" nodeTypeAlias="GeneralInterior">
