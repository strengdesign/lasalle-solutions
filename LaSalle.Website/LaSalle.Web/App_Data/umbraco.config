        <Item id="1783" key="f81677af-bca8-4444-b44a-f50d68c26e87" parentID="1131" level="4" creatorID="0" sortOrder="41" createDate="2016-08-10T13:34:28" updateDate="2016-08-10T13:43:05" nodeName="LaSalle Solutions Joins CrowdStrike’s Elevate Partner Program" urlName="lasalle-solutions-joins-crowdstrike-s-elevate-partner-program" path="-1,1056,1071,1131,1783" isDoc="" nodeType="1130" creatorName="Streng Design" writerName="tarek.wiley" writerID="6" template="1135" nodeTypeAlias="Item">
