@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Interior.cshtml";
}

<div class="interior-content with-rail">
	@CurrentPage.bodyText
	@{Html.RenderPartial("_Testimonials");}
</div> <!-- /interior-content -->

<div class="interior-rail">
	@{Html.RenderPartial("_SidebarTestimonialTags");}
	@CurrentPage.topContent
	@{Html.RenderPartial("_SidebarTestimonials");}
	@CurrentPage.bottomContent
</div> <!-- /interior-rail -->